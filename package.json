    "@types/leven": "^2.1.2",
    "@types/mysql": "^2.15.16",
    "@types/typescript": "^2.0.0",
    "chalk": "^4.1.0",
    "commander": "^6.2.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "leven": "^3.1.0",
    "log4js": "^6.3.0",
    "mongoose": "^5.11.4",
    "toml": "^3.0.0",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/chalk": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.9.0",
    "ajv": "^6.12.6",
    "cross-env": "^7.0.2",
